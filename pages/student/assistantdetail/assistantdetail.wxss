/* pages/student/assistantdetail/assistantdetail.wxss */

.avatar{
    height: 160px;
    display: flex;
    flex-direction: row;
    position: relative;
    overflow: hidden;
    z-index: 1;
    align-items: center;
}

.blur{
    filter: blur(30px);
    -webkit-filter: blur(30px);
    background-size: 100% 100%;
    width: 100%;
    height: 160px;
    overflow: hidden;
    position: absolute;
    align-self: center;
    z-index: 0;
    background-repeat: no-repeat;
}

